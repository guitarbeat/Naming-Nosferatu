{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": [
		"source/main.tsx"
	],
	"project": [
		"source/**/*.{js,jsx,ts,tsx}"
	],
	"ignore": [
		"**/*.md",
		"**/README.md",
		"**/IMPLEMENTATION.md",
		"config/postcss.config.js",
		"config/tailwind.config.js",
		"source/shared/utils/performanceMonitor.d.ts",
