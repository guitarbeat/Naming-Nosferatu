{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": ["source/main.tsx"],
	"project": ["source/**/*.{js,jsx,ts,tsx}"],
	"ignore": [
		"**/*.md",
		"**/README.md",
		"**/IMPLEMENTATION.md",
		"postcss.config.js",
		"tailwind.config.js",
		"source/types/shims.d.ts",
		"source/shared/utils/performanceMonitor.d.ts",
		"source/shared/components/NameSuggestionModal/NameSuggestionModal.d.ts"
	],
	"ignoreDependencies": [],
	"typescript": true,
	"paths": {
		"@/*": ["./source/*"],
		"@db": ["./supabase"],
		"@db/*": ["./supabase/*"],
		"@store": ["./source/store.ts"],
		"@types": ["./source/types.ts"],
		"@hooks": ["./source/hooks/index.ts"],
		"@hooks/*": ["./source/hooks/*"],
		"@utils": ["./source/utils/index.ts"],
		"@utils/*": ["./source/utils/*"],
		"@services": ["./source/services/index.ts"],
		"@services/*": ["./source/services/*"],
		"@styles": ["./source/styles/index.ts"],
		"@styles/*": ["./source/styles/*"],
		"@features": ["./source/features/index.ts"],
		"@features/*": ["./source/features/*"],
		"@core": ["./source/index.ts"],
		"@core/*": ["./source/*"],
		"@supabase/client": ["./source/services/supabase/client.ts"],
		"@supabase/types": ["./supabase/types.ts"]
	}
}
