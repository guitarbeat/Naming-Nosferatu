		"source/layout/Card/index.ts",
		"source/layout/Card/CardName.tsx",
		"source/layout/Card/CardStats.tsx"
	],
	"ignoreDependencies": [
		"@tailwindcss/postcss",
		"autoprefixer",
		"postcss",
		"postcss-discard-unused",
		"tailwindcss",
		"@radix-ui/react-slot"
	],
	"ignoreBinaries": [],
	"typescript": true,
	"ignoreExportsUsedInFile": true
}
