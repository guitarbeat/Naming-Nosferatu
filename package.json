{
  "name": "name-nosferatu",
  "version": "1.0.1",
  "description": "A React application for managing cat names and related data",
  "type": "module",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guitarbeat/name-nosferatu.git"
  },
  "bugs": {
    "url": "https://github.com/guitarbeat/name-nosferatu/issues"
  },
  "keywords": [
    "react",
    "cats",
    "names",
    "management",
    "tournament",
    "voting",
    "supabase",
    "vite"
  ],
  "author": {
    "name": "Aaron",
    "email": "aaron@example.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "build": "vite build --config config/vite.config.ts --outDir dist",
    "preview": "vite preview --config config/vite.config.ts",
    "test": "vitest run --config config/vitest.config.mjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "npm run lint:eslint && npm run lint:style && npm run lint:types",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:style:fix && npm run format",
    "lint:eslint": "eslint --config config/eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:eslint:fix": "eslint --config config/eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:style": "stylelint --config config/.stylelintrc.json \"src/**/*.css\"",
    "lint:style:fix": "stylelint --config config/.stylelintrc.json \"src/**/*.css\" --fix",
    "lint:types": "tsc --noEmit --project config/tsconfig.json",
    "check:css": "echo 'CSS analysis:' && purgecss --css 'src/**/*.module.css' --content 'src/**/*.{js,jsx,ts,tsx}' --output ./purgecss-report && node -e \"const fs=require('fs'),postcss=require('postcss'),discard=require('postcss-discard-unused');postcss([discard({kf:1,ff:1})]).process(fs.readFileSync('src/shared/components/Error/Error.module.css','utf8')).then(r=>console.log('CSS lines:',fs.readFileSync('src/shared/components/Error/Error.module.css','utf8').split('\\\\n').length,'â†’',r.css.split('\\\\n').length));\"",
    "check:unused": "ts-prune --project config/tsconfig.json",
    "check:deps": "knip --config config/knip.json",
    "check:deps:fix": "knip --config config/knip.json --fix",
    "check:all": "npm run lint && npm run check:css && npm run check:unused && npm run check:deps",
    "tsr": "cd config && tsr -p tsconfig.json '../src/main\\.tsx$' '../src/setupTests\\.ts$' '../src/shared/components/NameManagementView/NameManagementView\\.jsx$' '../src/features/tournament/components/SwipeMode/SwipeableNameCards\\.jsx$'",
    "tsr:write": "cd config && tsr -p tsconfig.json -w '../src/main\\.tsx$' '../src/setupTests\\.ts$' '../src/shared/components/NameManagementView/NameManagementView\\.jsx$' '../src/features/tournament/components/SwipeMode/SwipeableNameCards\\.jsx$'",
    "clean": "rm -rf build dist node_modules/.vite"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.8.5",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.87.1",
    "@tanstack/react-query": "^5.90.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.25",
    "lovable-tagger": "^1.1.13",
    "lucide-react": "^0.561.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-responsive-masonry": "^2.7.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.22",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "knip": "^5.73.1",
    "postcss": "^8.5.6",
    "postcss-discard-unused": "^7.0.4",
    "prettier": "^3.6.2",
    "purgecss": "^7.0.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "ts-prune": "^0.10.3",
    "tsr": "^1.3.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.15"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "webpack-dev-server": "^4.15.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16",
    "form-data": "^2.5.4",
    "tough-cookie": "^4.1.3",
    "fetch-blob": "^3.2.0"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/guitarbeat"
  },
  "files": [
    "build",
    "public",
    "src",
    "config",
    "scripts",
    "*.md",
    "*.json"
  ],
  "volta": {
    "node": "22.12.0",
    "npm": "10.9.0"
  }
}
