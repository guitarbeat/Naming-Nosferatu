{
  "name": "name-nosferatu",
  "version": "1.0.2",
  "description": "A React application for managing cat names and related data",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guitarbeat/name-nosferatu.git"
  },
  "bugs": {
    "url": "https://github.com/guitarbeat/name-nosferatu/issues"
  },
  "keywords": [
    "react",
    "cats",
    "names",
    "management",
    "tournament",
    "voting",
    "supabase",
    "vite"
  ],
  "author": {
    "name": "Aaron",
    "email": "aaron@example.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "build": "vite build --config config/vite.config.ts --outDir dist",
    "build:dev": "vite build --config config/vite.config.ts --mode development --outDir dist",
    "preview": "vite preview --config config/vite.config.ts",
    "test": "vitest run --config config/vitest.config.mjs",
    "lint": "pnpm run lint:biome && pnpm run lint:types",
    "lint:biome": "biome check src",
    "lint:fix": "biome check --write src",
    "lint:types": "tsc --noEmit --project config/tsconfig.json",
    "format": "biome format --write src",
    "check": "pnpm run lint && pnpm run check:deps && pnpm run check:unused && pnpm run check:limits",
    "check:limits": "node scripts/enforce-limits.js",
    "check:deps": "knip --config config/knip.json",
    "check:deps:fix": "knip --config config/knip.json --fix",
    "check:unused": "ts-prune --project config/tsconfig.json",
    "clean": "rm -rf build dist node_modules/.vite stats.html"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.8.7",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.16",
    "class-variance-authority": "^0.7.1",
    "framer-motion": "^12.24.7",
    "immer": "^10.0.3",
    "lovable-tagger": "^1.1.13",
    "lucide-react": "^0.562.0",
    "neverthrow": "^8.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.49.3",
    "react-router-dom": "^6.21.3",
    "ts-pattern": "^5.9.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "jsdom": "^27.4.0",
    "knip": "^5.80.0",
    "postcss": "^8.5.6",
    "postcss-discard-unused": "^7.0.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^4.1.18",
    "terser": "^5.44.1",
    "ts-prune": "^0.10.3",
    "tsr": "^1.3.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "form-data": "^2.5.4",
    "tough-cookie": "^4.1.3",
    "fetch-blob": "^3.2.0"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/guitarbeat"
  },
  "files": [
    "build",
    "public",
    "src",
    "config",
    "scripts",
    "*.md",
    "*.json"
  ],
  "volta": {
    "node": "22.12.0",
    "pnpm": "10.17.1"
  }
}
