{
  "name": "name-nosferatu",
  "version": "1.0.1",
  "description": "A React application for managing cat names and related data",
  "type": "module",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guitarbeat/name-nosferatu.git"
  },
  "bugs": {
    "url": "https://github.com/guitarbeat/name-nosferatu/issues"
  },
  "keywords": [
    "react",
    "cats",
    "names",
    "management",
    "tournament",
    "voting",
    "supabase",
    "vite"
  ],
  "author": {
    "name": "Aaron",
    "email": "aaron@example.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0 <21.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "build": "vite build --config config/vite.config.ts --outDir dist",
    "preview": "vite preview --config config/vite.config.ts",
    "test": "vitest run --config config/vitest.config.mjs",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format src",
    "format:check": "biome format src",
    "lint:types": "tsc --noEmit --project config/tsconfig.json",
    "workflow:check": "npm run lint",
    "workflow:fix": "npm run lint:fix",
    "check:css": "echo 'CSS analysis:' && purgecss --css 'src/**/*.module.css' --content 'src/**/*.{js,jsx,ts,tsx}' --output ./purgecss-report && node -e \\\"const fs=require('fs'),postcss=require('postcss'),discard=require('postcss-discard-unused');postcss([discard({kf:1,ff:1})]).process(fs.readFileSync('src/shared/components/Error/Error.module.css','utf8')).then(r=>console.log('CSS lines:',fs.readFileSync('src/shared/components/Error/Error.module.css','utf8').split('\\\\\\\\n').length,'â†’',r.css.split('\\\\\\\\n').length));\\\"",
    "check:unused": "ts-prune --project config/tsconfig.json",
    "check:deps": "knip --config config/knip.json",
    "check:deps:fix": "knip --config config/knip.json --fix",
    "check:all": "npm run lint && npm run check:css && npm run check:unused && npm run check:deps",
    "tsr": "cd config && tsr -p tsconfig.json '../src/main\\.tsx$' '../src/shared/components/ViewRouter/ViewRouter\\.tsx$' '../src/setupTests\\.ts$' '../src/features/tournament/Dashboard\\.tsx$' '../src/features/bongo/BongoPage\\.tsx$' '../src/features/tournament/TournamentSetup\\.tsx$' '../src/features/tournament/components/TournamentHeader/TournamentHeader\\.tsx$' '../src/features/tournament/Tournament\\.jsx$'",
    "tsr:write": "cd config && tsr -p tsconfig.json -w '../src/main\\.tsx$' '../src/shared/components/ViewRouter/ViewRouter\\.tsx$' '../src/setupTests\\.ts$' '../src/features/tournament/Dashboard\\.tsx$' '../src/features/bongo/BongoPage\\.tsx$' '../src/features/tournament/TournamentSetup\\.jsx$' '../src/features/tournament/components/TournamentHeader/TournamentHeader\\.tsx$' '../src/features/tournament/Tournament\\.jsx$'",
    "check:duplicates": "jscpd src --min-lines 5 --min-tokens 50 --format console",
    "check:duplicates:html": "jscpd src --min-lines 5 --min-tokens 50 --format html --output ./jscpd-report.html",
    "clean": "rm -rf build dist node_modules/.vite"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.8.5",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.90.16",
    "@trpc/client": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.25",
    "lovable-tagger": "^1.1.13",
    "lucide-react": "^0.562.0",
    "neverthrow": "^8.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-virtualized-auto-sizer": "^2.0.1",
    "react-window": "^2.2.3",
    "ts-pattern": "^5.9.0",
    "zod": "^4.3.4",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "knip": "^5.79.0",
    "postcss": "^8.5.6",
    "postcss-discard-unused": "^7.0.4",
    "prisma": "^7.2.0",
    "purgecss": "^7.0.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "ts-prune": "^0.10.3",
    "tsr": "^1.3.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "webpack-dev-server": "^4.15.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16",
    "form-data": "^2.5.4",
    "tough-cookie": "^4.1.3",
    "fetch-blob": "^3.2.0"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/guitarbeat"
  },
  "files": [
    "build",
    "public",
    "src",
    "config",
    "scripts",
    "*.md",
    "*.json"
  ],
  "volta": {
    "node": "22.12.0",
    "npm": "10.9.0"
  }
}
