{
	"name": "name-nosferatu",
	"version": "1.0.2",
	"description": "A React application for managing cat names and related data",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/guitarbeat/name-nosferatu.git"
	},
	"bugs": {
		"url": "https://github.com/guitarbeat/name-nosferatu/issues"
	},
	"keywords": [
		"react",
		"cats",
		"names",
		"management",
		"tournament",
		"voting",
		"supabase",
		"vite"
	],
	"author": {
		"name": "Aaron",
		"email": "aaron@example.com"
	},
	"license": "MIT",
	"packageManager": "pnpm@10.27.0",
	"engines": {
		"node": ">=20.19.0",
		"pnpm": ">=10.26.0"
	},
	"scripts": {
		"dev": "vite --config config/vite.config.ts",
		"build": "vite build --config config/vite.config.ts",
		"build:dev": "vite build --config config/vite.config.ts --mode development",
		"preview": "vite preview --config config/vite.config.ts",
		"lint": "pnpm run lint:full && pnpm run lint:types",
		"lint:full": "biome check source",
		"lint:types": "tsc --noEmit",
		"lint:fix": "biome check --write source",
		"format": "biome format --write source",
		"fix": "pnpm run lint:fix",
		"check": "pnpm run lint && pnpm run check:deps",
		"check:deps": "knip --config config/knip.json",
		"check:deps:fix": "knip --config config/knip.json --fix",
		"clean": "rm -rf build dist node_modules/.vite"
	},
	"dependencies": {
		"@hello-pangea/dnd": "^18.0.1",
		"@heroui/react": "^2.8.8",
		"@hookform/resolvers": "^5.2.2",
		"@radix-ui/react-slot": "^1.2.4",
		"@supabase/supabase-js": "^2.93.3",
		"@tanstack/react-query": "^5.90.20",
		"@vercel/analytics": "^1.6.1",
		"class-variance-authority": "^0.7.1",
		"framer-motion": "^12.33.0",
		"lovable-tagger": "^1.1.13",
		"lucide-react": "^0.563.0",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"react-hook-form": "^7.71.1",
		"react-router-dom": "^7.13.0",
		"zod": "^4.3.6",
		"zustand": "^5.0.10"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.14",
		"@tailwindcss/postcss": "^4.1.18",
		"@types/node": "^25.2.2",
		"@types/react": "^19.2.10",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.3",
		"autoprefixer": "^10.4.24",
		"knip": "^5.83.1",
		"postcss": "^8.5.6",
		"postcss-discard-unused": "^7.0.4",
		"tailwindcss": "^4.1.18",
		"terser": "^5.46.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vite-plugin-pwa": "^1.2.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version",
			"last 1 edge version"
		]
	},
	"overrides": {
		"nth-check": "^2.1.1",
		"form-data": "^2.5.4",
		"tough-cookie": "^4.1.3",
		"fetch-blob": "^3.2.0",
		"undici": "^6.23.0",
		"tar": "^7.5.4"
	},
	"pnpm": {
		"overrides": {
			"@isaacs/brace-expansion": ">=5.0.1",
			"tar@<=7.5.2": ">=7.5.3",
			"tar@<=7.5.3": ">=7.5.4",
			"undici@<5.29.0": ">=5.29.0",
			"undici@<6.23.0": ">=6.23.0",
			"undici@>=4.5.0 <5.28.5": ">=5.28.5",
			"undici": "^6.23.0"
		}
	},
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/guitarbeat"
	},
	"files": [
		"build",
		"public",
		"source",
		"scripts",
		"*.md",
		"*.json"
	],
	"volta": {
		"node": "20.19.6",
		"pnpm": "10.27.0"
	}
}
