{
  "name": "name-nosferatu",
  "version": "1.0.1",
  "description": "A React application for managing cat names and related data",
  "type": "module",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guitarbeat/name-nosferatu.git"
  },
  "bugs": {
    "url": "https://github.com/guitarbeat/name-nosferatu/issues"
  },
  "keywords": [
    "react",
    "cats",
    "names",
    "management",
    "tournament",
    "voting",
    "supabase",
    "vite"
  ],
  "author": {
    "name": "Aaron",
    "email": "aaron@example.com"
  },
  "license": "MIT",
  "engines": {
    "node": "20.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite --config config/vite.config.ts",
    "build": "vite build --config config/vite.config.ts && node scripts/postbuild-fix-react.js",
    "preview": "vite preview --config config/vite.config.ts",
    "test": "vitest run --coverage --config config/vitest.config.mjs",
    "lint": "eslint --config config/eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\" --fix && stylelint --config config/.stylelintrc.json \"src/**/*.css\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "check": "npm run lint && prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\" && tsc --noEmit",
    "ci": "npm run check && npm test && npm run build",
    "clean": "rm -rf build dist node_modules/.vite"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.83.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "class-variance-authority": "^0.7.1",
    "dotenv": "^17.2.3",
    "lovable-tagger": "^1.1.11",
    "lucide-react": "^0.554.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "stylelint-order": "^7.0.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.10",
    "autoprefixer": "^10.4.22",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "glob": "^12.0.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "npm-check-updates": "^19.1.2",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.5",
    "stylelint": "^16.25.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "supabase": "^2.58.5",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^4.0.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "webpack-dev-server": "^4.15.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/guitarbeat"
  },
  "files": [
    "build",
    "public",
    "src",
    "config",
    "scripts",
    "*.md",
    "*.json"
  ],
  "volta": {
    "node": "22.12.0",
    "npm": "10.9.0"
  }
}
