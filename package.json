{
	"name": "name-nosferatu",
	"version": "1.0.2",
	"description": "A React application for managing cat names and related data",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/guitarbeat/name-nosferatu.git"
	},
	"bugs": {
		"url": "https://github.com/guitarbeat/name-nosferatu/issues"
	},
	"keywords": [
		"react",
		"cats",
		"names",
		"management",
		"tournament",
		"voting",
		"supabase",
		"vite"
	],
	"author": {
		"name": "Aaron",
		"email": "aaron@example.com"
	},
	"license": "MIT",
	"packageManager": "pnpm@10.27.0",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=9.0.0"
	},
	"scripts": {
		"dev": "vite --config vite.config.ts",
		"build": "vite build --config vite.config.ts --outDir dist",
		"build:dev": "vite build --config vite.config.ts --mode development --outDir dist",
		"preview": "vite preview --config vite.config.ts",
		"lint": "pnpm run lint:quick && pnpm run lint:types",
		"lint:quick": "oxlint",
		"lint:full": "biome check source",
		"lint:fix": "biome check --write source",
		"lint:types": "tsc --noEmit --project tsconfig.base.json",
		"format": "biome format --write source",
		"fix": "pnpm run lint:fix",
		"check": "pnpm run lint && pnpm run check:deps",
		"check:deps": "knip --config knip.json",
		"check:deps:fix": "knip --config knip.json --fix",
		"check:outdated": "pnpm outdated",
		"check:audit": "pnpm audit",
		"update:check": "pnpm outdated --format table",
		"clean": "del /q build dist node_modules\\.vite 2>nul || rmdir /s /q build dist node_modules\\.vite 2>nul || echo No files to clean",
		"vercel:dev": "vercel dev",
		"optimize:sounds": "node scripts/optimize-sounds.js"
	},
	"dependencies": {
		"@hello-pangea/dnd": "^18.0.1",
		"@hookform/resolvers": "^5.2.2",
		"@radix-ui/react-slot": "^1.2.4",
		"@supabase/supabase-js": "^2.90.0",
		"@tanstack/react-query": "^5.90.16",
		"class-variance-authority": "^0.7.1",
		"framer-motion": "^12.24.10",
		"lucide-react": "^0.562.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-hook-form": "^7.49.3",
		"react-router-dom": "^7.12.0",
		"zod": "^4.3.5",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@tailwindcss/postcss": "^4.1.18",
		"@types/node": "^25.0.3",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"knip": "^5.80.0",
		"lefthook": "^2.0.13",
		"oxlint": "^1.38.0",
		"postcss": "^8.5.6",
		"postcss-discard-unused": "^7.0.4",
		"stylelint": "^16.26.1",
		"stylelint-config-standard": "^39.0.1",
		"tailwindcss": "^4.1.18",
		"terser": "^5.44.1",
		"typescript": "^5.9.3",
		"vercel": "^50.3.2",
		"vite": "^7.3.1",
		"vite-plugin-pwa": "^1.2.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version",
			"last 1 edge version"
		]
	},
	"overrides": {
		"nth-check": "^2.1.1",
		"form-data": "^2.5.4",
		"tough-cookie": "^4.1.3",
		"fetch-blob": "^3.2.0"
	},
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/guitarbeat"
	},
	"files": [
		"build",
		"public",
		"source",
		"scripts",
		"*.md",
		"*.json"
	],
	"volta": {
		"node": "22.12.0",
		"pnpm": "10.27.0"
	}
}
